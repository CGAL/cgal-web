, author       = {Herv{\'e} Br{\"o}nnimann and Lutz Kettner and
                  Stefan Schirra and Remco Veltkamp}
, title        = {Applications of the Generic Programming Paradigm
                  in the Design of {CGAL}}
, booktitle    = {Generic Programming---Proceedings of a Dagstuhl Seminar}
, editor       = {M. Jazayeri and R. Loos and D. Musser}
, series       = {LNCS 1766}
, publisher    = {Springer-Verlag}
, pages        = {206--217}
, year         = 2000
, url = "http://www.mpi-sb.mpg.de/~kettner/pub/cgal_generic_lncs_00_a.html"
, abstract     = {
        We report on the use of the generic programming paradigm in
	the computational geometry algorithms library CGAL. The
	parameterization of the geometric algorithms in CGAL enhances
	flexibility and adaptability and opens an easy way for abolishing
	precision and robustness problems by exact but nevertheless efficient
	computation. Furthermore we discuss circulators, which are an
	extension of the iterator concept to circular structures. Such
	structures arise frequently in geometric computing.}
, keyword = "_Design_"
}

